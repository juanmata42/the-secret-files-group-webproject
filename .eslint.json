{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "airbnb-base",
    "react-app",
    "react-app/jest",
    "prettier"
  ],
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 12,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "strict": "off",
    "no-console": "off",
    "import/prefer-default-export": "off",
    "react/react-in-jsx-scope": "off"
  }
}
